{
  "name": "at-your-service",
  "version": "0.0.14",
  "description": "Generate OpenAPI 3.1 specifications on the frontend with zero config via a service worker proxy",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "at-your-service": "./dist/cli.js"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": "https://github.com/AndrewWalsh/at-your-service",
  "scripts": {
    "dev": "vite -c ./vite.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "vite build -c ./vite.config.ts && npm run build:cli && npm run build:sw",
    "format": "prettier ./src --write",
    "build:cli": "cpy src/_not_exported/cli.js dist --flat",
    "build:sw": "cpy src/_not_exported/at-your-service-sw.js dist --flat",
    "preview": "vite preview -c ./vite.config.ts",
    "prepublish": "npm run build && cd demo && npm i && npm run build && cd .."
  },
  "keywords": [],
  "author": "Andrew Walsh",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@seriousme/openapi-schema-validator": "^2.1.0",
    "@types/yargs": "^17.0.13",
    "@vitest/coverage-c8": "^0.25.1",
    "prettier": "2.7.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.4.1",
    "vite": "^3.2.2",
    "vitest": "^0.24.5"
  },
  "peerDependencies": {
    "alterschema": "^1.1.1",
    "cpy-cli": "^4.2.0",
    "genson-js": "^0.0.8",
    "lodash": "^4.17.21",
    "openapi3-ts": "^3.1.1",
    "quicktype-core": "^6.1.0",
    "redom": "^3.29.1",
    "store2": "^2.14.2",
    "strict-event-emitter": "^0.2.8",
    "type-fest": "^3.1.0",
    "validator": "^13.7.0",
    "yargs": "^17.6.2",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": ">=14"
  }
}
