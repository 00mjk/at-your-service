{
  "name": "at-your-service",
  "version": "0.0.17",
  "description": "An easy-install development tool leveraging service workers as proxies to generate OpenAPI 3.1 specifications and models/serializers in 10+ languages directly on the frontend",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "at-your-service": "./dist/cli.js"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": "https://github.com/AndrewWalsh/at-your-service",
  "scripts": {
    "dev": "vite -c ./vite.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "vite build -c ./vite.config.ts && npm run build:cli && npm run build:sw",
    "format": "prettier ./src --write",
    "build:cli": "cpy src/_not_exported/cli.js dist --flat",
    "build:sw": "cpy src/_not_exported/at-your-service-sw.js dist --flat",
    "preview": "vite preview -c ./vite.config.ts",
    "prepublish": "npm run build && cd demo && npm i && npm run build && cd .."
  },
  "keywords": [
    "openapi",
    "swagger",
    "api",
    "documentation",
    "service",
    "service worker",
    "service-worker",
    "proxy",
    "openapi-generation",
    "generation",
    "spec",
    "gen",
    "typescript",
    "dev",
    "tool",
    "developer experience",
    "observability",
    "openapi-spec",
    "openapi-gen",
    "openapi-3.1",
    "json",
    "schema",
    "json-schema",
    "codegen",
    "ts",
    "javascript",
    "browser",
    "frontend"
  ],
  "author": "Andrew Walsh",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@seriousme/openapi-schema-validator": "^2.1.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/yargs": "^17.0.13",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.1",
    "prettier": "2.7.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tslib": "^2.4.1",
    "vite": "^3.2.2",
    "vitest": "^0.24.5"
  },
  "peerDependencies": {
    "alterschema": "^1.1.1",
    "cpy-cli": "^4.2.0",
    "genson-js": "^0.0.8",
    "lodash": "^4.17.21",
    "openapi3-ts": "^3.1.1",
    "quicktype-core": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "store2": "^2.14.2",
    "strict-event-emitter": "^0.2.8",
    "type-fest": "^3.1.0",
    "validator": "^13.7.0",
    "yargs": "^17.6.2",
    "yup": "^0.32.11",
    "@geist-ui/core": "^2.3.8",
    "react-syntax-highlighter": "^15.5.0"
  },
  "bugs": "https://github.com/AndrewWalsh/at-your-service/issues",
  "engines": {
    "node": ">=14"
  }
}
