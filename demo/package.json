{
  "name": "at-your-service-demo",
  "description": "A test environment for at-your-service",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite -c vite.config.ts --port 8081\" \"node server.js\"",
    "build": "vite build",
    "build:docs": "vite -c ./vite.config.build.ts build --outDir ../docs --emptyOutDir",
    "test": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "at-your-service": "file:..",
    "msw": "^0.49.0",
